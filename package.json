{
  "name": "remixcipe",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#*": "./*"
  },
  "scripts": {
    "build": "remix vite:build",
    "dev": "node ./server.js",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier  --ignore-path .gitignore --check .",
    "prepare": "husky",
    "test": "vitest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --ignore-path .gitignore --cache --fix",
    "*.{js,jsx,ts,tsx,css}": "prettier --ignore-path .gitignore --write"
  },
  "dependencies": {
    "@remix-run/express": "^2.13.1",
    "@remix-run/node": "^2.13.1",
    "@remix-run/react": "^2.13.1",
    "address": "^2.0.3",
    "chalk": "^5.3.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.1",
    "get-port": "^7.1.0",
    "glob": "^11.0.0",
    "isbot": "^4.1.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.7.0",
    "remix-auth-form": "^1.5.0",
    "remix-auth-github": "^2.0.0",
    "remix-utils": "^7.7.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@playwright/test": "^1.48.2",
    "@remix-run/dev": "^2.13.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.8.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "chokidar": "^4.0.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "execa": "^9.5.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "remix-flat-routes": "^0.6.5",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^2.1.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
